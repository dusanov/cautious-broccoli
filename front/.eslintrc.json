{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
            "endOfLine": "auto"
            }
        ],
        "@typescript-eslint/strict-boolean-expressions": "off"
    }
}
